{
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "format": "prettier --write --ignore-path .gitignore {**,**/**}/*.{vue,ts,json,html,md}",
    "format:check": "prettier --check --ignore-path .gitignore {**,**/**}/*.{vue,ts,json,html,md}",
    "lint": "eslint --ignore-path .gitignore --ext vue,ts,js .",
    "lint:fix": "eslint --ignore-path .gitignore --ext vue,ts,js --fix .",
    "postinstall": "husky install"
  },
  "dependencies": {
    "vue": "^3.0.5",
    "vue-router": "^4.0.10"
  },
  "devDependencies": {
    "@types/node": "^15.14.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.0.5",
    "@vueuse/head": "^0.6.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.12.1",
    "husky": "^7.0.0",
    "prettier": "^2.3.2",
    "sass": "^1.35.1",
    "typescript": "^4.3.2",
    "vite": "^2.3.7",
    "vite-ssr": "^0.10.6",
    "vue-eslint-parser": "^7.6.0",
    "vue-tsc": "^0.0.24"
  }
}
